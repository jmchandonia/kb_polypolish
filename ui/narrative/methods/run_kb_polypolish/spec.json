{
    "ver": "0.0.1",
    "authors": [
        "jmc"
    ],
    "contact": "http://kbase.us/contact-us/",
    "categories": ["active", "assembly"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "input_assembly",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types" : [ "KBaseGenomeAnnotations.Assembly" ]
            }
        },
        {
            "id": "input_reads_library",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseAssembly.SingleEndLibrary", "KBaseFile.SingleEndLibrary", "KBaseAssembly.PairedEndLibrary", "KBaseFile.PairedEndLibrary"]
            }
        },
        {
            "id": "fraction_invalid",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.2" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
		"min_float" : 0.0,
		"max_float" : 1.0
            }
        },
        {
            "id": "fraction_valid",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.5" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
		"min_float" : 0.0,
		"max_float" : 1.0
            }
        },
        {
            "id": "max_errors",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "10" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		"min_int" : 0
            }
        },
        {
            "id": "min_depth",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "5" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		"min_int" : 0
            }
        },
        {
            "id": "output_assembly",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["polished.contigs"],
            "field_type": "text",
            "text_options": {
                "valid_ws_types" : [ "KBaseGenomeAnnotations.Assembly" ],
                "is_output_name":true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_polypolish",
            "method": "run_kb_polypolish",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "input_assembly",
                    "target_property": "input_assembly",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "input_reads_library",
                    "target_property": "input_reads_library",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "fraction_invalid",
                    "target_property": "fraction_invalid"
                },{
                    "input_parameter": "fraction_valid",
                    "target_property": "fraction_valid"
                },{
                    "input_parameter": "max_errors",
                    "target_property": "max_errors"
                },{
                    "input_parameter": "min_depth",
                    "target_property": "min_depth"
                },{
                    "input_parameter": "output_assembly",
                    "target_property": "output_assembly"
		}
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
